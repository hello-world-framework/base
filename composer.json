{
    "name": "hello-world-framework/base",
    "type": "project",
    "description": "Base configuration provider for the HelloWorld Framework(a small dynamic env unit)",
    "keywords": [
        "base",
        "app-base",
        "hello-world-framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Md. Reyadus Salahin",
            "email": "reyadussalahin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "rs-world/instantiator": "1.*"
    },
    "autoload": {
        "psr-4": {
            "HW\\Base\\": "src/HW/Base/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Instantiators\\": "src/Instantiators"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
